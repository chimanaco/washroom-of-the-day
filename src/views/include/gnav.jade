mixin gnav(obj)
	-var menu = { 'List': '/#/users', 'ListUser': '/#/users2', 'Edit': '/#/users/new', 'Map': '/#/map', 'Stats': '/#/stats', 'Visualizer': '/#/visualizer' };
	-var selected = obj

	ul(class='nav nav-tabs')
		each val, key in menu
			li
				if selected === key
					li.active(role="presentation")
						a(ng-href=val, title=key)= key
				else
					li(role="presentation")
						a(ng-href=val, title=key)= key